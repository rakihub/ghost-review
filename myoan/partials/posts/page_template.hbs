<article class="post-section g-grid-template g-parent-full g-child-site">
	{{#match @page.show_title_and_feature_image}}
	{{>posts/header}}
	{{/match}}
	<div class="post-content g-grid-template g-parent-full g-child-regular{{^if access}}{{#if html}} is-teaser{{/if}}{{/if}}">
		{{#if access}}
		{{content}}
		{{else}}
		{{#if html}}{{{html}}}{{/if}}
		{{/if}}
	</div>
	{{^if access}}
	{{>members/cta}}
	{{/if}}
</article>
{{>members/additional is_footer=true}}